    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start",
    "dev-full": "concurrently --names \"API,EXPO\" --prefix-colors \"bgBlue,bgGreen\" \"npm run server\" \"npm run dev\"",
