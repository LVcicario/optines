    "stop-server": "taskkill /F /IM node.exe 2>nul || echo 'Aucun processus Node.js trouv√©'",
    "restart": "npm run stop-server && npm run start",
    "test": "jest",
    "check-modifications": "node scripts/check-modifications.js",
